    "version": "0.2.0",
    "configurations": [
        {
            "name": "clang++.exe - Build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
            "args": [
                "-I\"C:/Program Files/LLVM/lib/clang/11.0.0/include/immer-0.6.2/\"",
                "-I\"C:/Program Files/LLVM/lib/clang/11.0.0/include/immer-0.6.2/immer/\"",
                "-I\"C:/Program Files/LLVM/lib/clang/11.0.0/include/immer-0.6.2/immer/detail/rbts/\"",
                "-lC:/Users/TFran/VisionCode/src/Libs/SDL2main",
                "-lC:/Users/TFran/VisionCode/src/Libs/SDL2",
                "-lC:/Users/TFran/VisionCode/src/Libs/SDL2_ttf"
                    ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb",
            "miDebuggerPath": "\\usr\\bin\\lldb-mi.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: clang.exe build active file"
        }
    ]
}